# Telegram Bot Token (get from @BotFather)
TELEGRAM_BOT_TOKEN=

# iikoServer API (полная номенклатура, включая товары на складе)
IIKO_SERVER_URL=https://bufet-17-co.iiko.it
IIKO_SERVER_LOGIN=ваш_логин
IIKO_SERVER_PASSWORD=ваш_пароль

# Для создания поставок (обязательно при IIKO_READ_ONLY=false):
# IIKO_DEFAULT_STORE_ID=1239d270-1bbe-f64f-b7ea-5f00518ef508
# IIKO_DEFAULT_COUNTERAGENT_ID=uuid_контрагента_поставщика
# Взять ID в iiko Office: Подразделения и Контрагенты

# Склады для выбора (если API не возвращает). Формат: uuid:Название,uuid2:Название2
# Известные из incomingInvoice.xml (названия можно уточнить в iiko Office):
# IIKO_WAREHOUSES=1239d270-1bbe-f64f-b7ea-5f00518ef508:Bufet 17 (Основной склад),bc65b2d2-f440-4c56-b1cb-513775216d49:Bufet 17 (Склад Gonzo Gaming),d4a7caed-7e9d-4be0-a2f2-d1d74f1a6e80:Bufet 17 (Услуги),0aba85a4-1f6c-4fdf-bdcc-e02f2b323654:Bufet 17 (Хоз товары склад)

# Режим только чтение (true = не создавать поставки)
IIKO_READ_ONLY=true
